public class quake
{
   public static void main(String [] args)
   {
       int richter;
       
       richter = 2;
       
       
        if (richter >=0)
            if (richter <=4)
                System.out.println("Harmless");
            else
                System.out.println("Harmful");
        else
            System.out.println("No negative");
            
    }  
}
